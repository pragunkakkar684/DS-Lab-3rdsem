#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ecd232db520 .scope module, "tb_priority_encoder_16to4" "tb_priority_encoder_16to4" 2 3;
 .timescale -9 -12;
v0x5ecd233051f0_0 .var "in", 15 0;
v0x5ecd233052e0_0 .net "out", 3 0, v0x5ecd233050b0_0;  1 drivers
S_0x5ecd232b3df0 .scope module, "uut" "priority_encoder_16to4" 2 9, 3 1 0, S_0x5ecd232db520;
 .timescale -9 -12;
    .port_info 0 /INPUT 16 "in";
    .port_info 1 /OUTPUT 4 "out";
v0x5ecd232b57b0_0 .var "found", 0 0;
v0x5ecd232b5ba0_0 .var/i "i", 31 0;
v0x5ecd23304ff0_0 .net "in", 15 0, v0x5ecd233051f0_0;  1 drivers
v0x5ecd233050b0_0 .var "out", 3 0;
E_0x5ecd232d79b0 .event edge, v0x5ecd23304ff0_0, v0x5ecd232b57b0_0;
    .scope S_0x5ecd232b3df0;
T_0 ;
    %wait E_0x5ecd232d79b0;
    %pushi/vec4 15, 15, 4;
    %store/vec4 v0x5ecd233050b0_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ecd232b57b0_0, 0, 1;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x5ecd232b5ba0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5ecd232b5ba0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5ecd23304ff0_0;
    %load/vec4 v0x5ecd232b5ba0_0;
    %part/s 1;
    %load/vec4 v0x5ecd232b57b0_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5ecd232b5ba0_0;
    %parti/s 4, 0, 2;
    %store/vec4 v0x5ecd233050b0_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ecd232b57b0_0, 0, 1;
T_0.2 ;
    %load/vec4 v0x5ecd232b5ba0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x5ecd232b5ba0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5ecd232db520;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "priority_encoder_16to4_tb.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5ecd232db520 {0 0 0};
    %vpi_call 2 18 "$display", "Time\011      in                 | out" {0 0 0};
    %vpi_call 2 19 "$display", "-------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5ecd233051f0_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 21 "$display", "%0t\011 %b | %b", $time, v0x5ecd233051f0_0, v0x5ecd233052e0_0 {0 0 0};
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0x5ecd233051f0_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 22 "$display", "%0t\011 %b | %b", $time, v0x5ecd233051f0_0, v0x5ecd233052e0_0 {0 0 0};
    %pushi/vec4 2, 0, 16;
    %store/vec4 v0x5ecd233051f0_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 23 "$display", "%0t\011 %b | %b", $time, v0x5ecd233051f0_0, v0x5ecd233052e0_0 {0 0 0};
    %pushi/vec4 128, 0, 16;
    %store/vec4 v0x5ecd233051f0_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 24 "$display", "%0t\011 %b | %b", $time, v0x5ecd233051f0_0, v0x5ecd233052e0_0 {0 0 0};
    %pushi/vec4 4096, 0, 16;
    %store/vec4 v0x5ecd233051f0_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 25 "$display", "%0t\011 %b | %b", $time, v0x5ecd233051f0_0, v0x5ecd233052e0_0 {0 0 0};
    %pushi/vec4 65535, 0, 16;
    %store/vec4 v0x5ecd233051f0_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 26 "$display", "%0t\011 %b | %b", $time, v0x5ecd233051f0_0, v0x5ecd233052e0_0 {0 0 0};
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
